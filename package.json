{
  "name": "heimdallr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reinstall": "rm -rf ./node_modules && rm -rf ./apps/telu-app/ios/Pods && yarn install",
    "postinstall": "yarn run cp:node_modules && yarn run setup-ios-permissions && yarn run pod-install && husky install",
    "setup-ios-permissions": "react-native setup-ios-permissions",
    "pod-install": "nx run-many --targets=pod-install --parallel=false",
    "lint": "nx run-many --targets=lint,stylelint,type-check --projects=\\*-app",
    "test": "cd ./__tests__/ && jest ./src/** && cd ..",
    "generate:assets": "node ./bin/generate-assets-import",
    "i18n-helper": "bash ./bin/i18n/helper.sh",
    "build": "bash ./bin/build_debug.sh",
    "release:android": "bash ./bin/distribute/release_android.sh",
    "codepush:deploy": "bash ./bin/distribute/codepush_deploy.sh",
    "cp:node_modules": "cp -rf ./__node_modules__/react-native-callkeep/* ./node_modules/react-native-callkeep && cp -rf ./__node_modules__/@react-navigation/* ./node_modules/@react-navigation && cp -rf ./__node_modules__/react-native-picker/* ./node_modules/react-native-picker && cp -rf ./__node_modules__/react-native-background-timer/* ./node_modules/react-native-background-timer && cp -rf ./__node_modules__/@types/* ./node_modules/@types",
    "nx:dep-graph": "nx dep-graph",
    "upgrade:pjsip": "yarn up react-native-pjsip -R",
    "upgrade:esim": "yarn up @heimdallr-native-esim -R",
    "upgrade:vpn": "yarn up @heimdallr-native-vpn -R",
    "git:config-commit-template": "cp .gitmessage.txt ~/.gitmessage.txt && git config --global commit.template ~/.gitmessage.txt && git config --global --add commit.cleanup strip",
    "test:build": "nx start telu-app --reset-cache"
  },
  "reactNativePermissionsIOS": [
    "Notifications",
    "Camera",
    "Microphone",
    "LocationWhenInUse",
    "PhotoLibrary",
    "PhotoLibraryAddOnly"
  ],
  "dependencies": {
    "@heimdallr-native-esim": "git+ssh://git@github.com:RDTel25/heimdallr-native.git#feat/esim",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "axios": "1.3.6",
    "buffer": "6.0.3",
    "events": "3.3.0",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-callkeep": "4.1.0",
    "react-native-fbsdk-next": "11.2.1",
    "react-native-permissions": "3.8.0",
    "react-native-pjsip": "git+ssh://git@github.com:RDTel25/react-native-pjsip.git",
    "rxjs": "6.6.3",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/runtime": "7.21.0",
    "@google-cloud/local-auth": "2.1.0",
    "@nrwl/jest": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/react-native": "15.9.2",
    "@react-native-community/eslint-config": "3.2.0",
    "@styled/typescript-styled-plugin": "1.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@tsconfig/react-native": "2.0.3",
    "@types/events": "3.0.0",
    "@types/jest": "29.5.0",
    "@types/react": "18.0.33",
    "@types/react-native": "0.71.6",
    "@types/react-native-background-timer": "2.0.0",
    "@types/react-native-vector-icons": "6.4.13",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "5.2.1",
    "@types/styled-system": "5.1.16",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "babel-jest": "29.5.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.38.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "metro-react-native-babel-preset": "0.73.9",
    "nx": "15.9.2",
    "prettier": "2.8.7",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.1.0",
    "stylelint-react-native": "2.2.0",
    "ts-jest": "29.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
   "paths": {
      "@heimdallr/services*": ["libs/services/src*"],
      "@/app-config": ["./apps/telu-app/app.config.js"],
    }
}
